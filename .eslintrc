{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaVersion": 2017
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "plugins": ["babel", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "linebreak-style": 0,
    "arrow-parens": 0
  }
}
